<%= stylesheet_link_tag "bootstrap" %>
<%= stylesheet_link_tag "signin" %>

<%=form_tag accountmanagement_createaccount_path, :class => 'form-signin' do%>
<%= link_to "Back", accountmanagement_path %>
<%= form_for User.new do |f| %>
<h2 class="form-signin-heading">Create a new Account</h2>
	<%=f.label :email %>
	<%=f.text_field :email, :class => 'form-control' %>
	<br>
	<%=f.label :password %>
	<%=f.password_field :password, :class => 'form-control' %>
	<%=f.label :password_confirmation %>
	<%=f.password_field :password_confirmation, :class => 'form-control' %>
	<br>
	<%=f.submit "Create Account", { :class => "btn btn-primary", :controller => 'accountmanagement', :action => 'createaccount'} %>
<% end %>
<%end%>