<thead>
    <tr>
        <th>Name</th>
        <th>Created At</th>
        <th>Updated At</th>
        <th>Edit</th>
        <th>Delete</th>
        <th colspan="10"></th>
    </tr>
</thead>
<tbody id="scenarios-data">
    <% if @families %>
        <% @families.each do |family| %>
	  <tr>
	    <td><%= family.encrypted_name %></td>
	    <td><%= family.created_at %></td>
	    <td><%= family.updated_at %></td>
	    <td><%= link_to 'Edit', edit_family_path(family) %></td>
	    <td><%= link_to 'Delete', family_path(family),
		    method: :delete, data: { confirm: 'Are you sure?' } %></td>
	   </tr>
      <% end %>
    <% end %>
</tbody>

